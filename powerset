#include<bits/stdc++.h>
using namespace std;
int main()
{
   int n,a[10];
   cin>>n;
    for(int i=0;i<n;i++)
        cin>>a[i];
   for(int i=0;i<n;i++)
   {
   int c=0,a1=1, sum=0;
   if(a[i]==c)
   sum+=c;
   else if(a[i]==a1)
   sum-=a1;
   }
   bool res;
   res=(sum%360==0)?yes:no;
   cout<<res;
   return 0;
}
