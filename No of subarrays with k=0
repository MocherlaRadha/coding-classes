//coding-classes
#include <iostream>
using namespace std;

int main(){
int n;
int a[n];
for(int i=0;i<n;i++)
{
cin>>a[i];
}
int pre[7]={0};
pre[0]=a[0];
int c=0,k=0;
for (int i=0;i<n-1;i++)
{
pre[i]=pre[i-1]+a[i];
}
map<int,int>m;
m.insert({0,1});
for(j=0;j<n-1;j++)
{
if(pre[j]-k in m)
{
c+=m[pre[j]-k];
}
else
m[pre[j]]+=1;
}
cout<<c;
	return 0;
}
